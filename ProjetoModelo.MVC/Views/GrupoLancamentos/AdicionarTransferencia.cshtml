
@model Moneta.Application.ViewModels.TransferenciaGrupoLancamentoViewModel

@{
    ViewBag.Title = "Consultar Transferência";
}

<h2>@ViewBag.Title</h2>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

        <div class="form-group">
            @Html.LabelFor(model => model.TransferenciaId, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.TransferenciaId, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.TransferenciaId, "", new { @class = "text-danger" })
                @Html.HiddenFor(model => model.GrupoLancamentoId)
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Consultar" class="btn btn-primary" />
                <span class="btn btn-default" onclick="javascript: window.history.back();">Cancelar</span>
            </div>
        </div>
    </div>
}